{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Medico</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombrecompleto</th>
                <td>{{ entity.nombreCompleto }}</td>
            </tr>
            <tr>
                <th>Titulosespecialidad</th>
                <td>{{ entity.titulosEspecialidad }}</td>
            </tr>
            <tr>
                <th>Pathfirma</th>
                <td>{{ entity.pathFirma }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('medico') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('medico_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
